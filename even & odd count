T = int(input())  # Number of test cases

for _ in range(T):
    N = int(input())
    odd_count = 0
    even_count = 0
    
    for i in range(1, N + 1):
        if N % i == 0:  # i is a divisor of N
            if i % 2 == 0:
                even_count += 1
            else:
                odd_count += 1

    print(odd_count, even_count)
